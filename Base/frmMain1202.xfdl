this.ivMainLogo_onclick = function(obj:nexacro.ImageViewer,e:nexacro.ClickEventInfo)
{
	// 메인화면으로 돌아가기
	this.parent.parent.divCenter.url = "Base::frmMain1203.xfdl"; 
};


this.btnMenu_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.parent.parent.divCenter.url = "Base::frmProduct1200.xfdl"; 
};

this.btnSirenOrder_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var statusAuth = nexacro.getApplication().appStatusAuth;
	if(statusAuth == "false" || statusAuth == false){
		this.alert("로그인해야 사용할 수 있습니다.");
		return;
	}

	var isInOrder = this.fvApp.gdsAuth.getColumn(0, "IS_IN_ORDER");
	if(isInOrder == null || isInOrder == "undefined"){
		alert("사이렌 오더 주문 시 이용할 수 있습니다.");
		return;
	}
	
	this.parent.parent.divCenter.url = "Order::frmCust1210.xfdl"; 
};

this.btnStore_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.parent.parent.divCenter.url = "Base::frmStore1200.xfdl"; 
};
