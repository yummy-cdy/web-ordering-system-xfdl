/************************************************************************************************
 * FORM 변수 선언 영역
 ************************************************************************************************/
this.fvApp = nexacro.getApplication(); 
 
this.fn_onmouseenter = function (obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	obj.background="mistyrose";
};

this.fn_onmouseleave = function (obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	obj.background = "none no-repeat left top";
};

/************************************************************************************************
 * FORM EVENT 영역(onload, onbeforeclose)
